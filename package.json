{
  "name": "vue-ssr-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/app.js",
    "serve": "concurrently -r -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:ts": "tsc -w -p server",
    "watch:node": "nodemon -w server/ dist/app.js",
    "serve:client": "vue-cli-service serve",
    "build": "npm run build:ts && npm run build:server && npm run build:client -- --no-clean",
    "build:ts": "rimraf -f dist && tsc -p server",
    "build:server": "cross-env SSR_TARGET=server vue-cli-service build",
    "build:client": "cross-env SSR_TARGET=client vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-server-renderer": "^2.6.10"
  },
  "devDependencies": {
    "@types/express-to-koa": "^1.0.0",
    "@types/jest": "^23.1.4",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/koa-webpack": "^5.0.1",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-pwa": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "express-to-koa": "^1.2.0",
    "koa-webpack": "^5.2.3",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.5.21",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.2.0"
  }
}
